					<e:Label x="270" top="0" bottom="0" textAlign="left" size="34" verticalAlign="middle" text="{data.creator}" width="560" lineSpacing="10"/>
				</e:Group>
				<e:Group width="860" height="126" x="213" y="242">
					<e:Rect left="0" right="0" top="0" bottom="0" ellipseWidth="20" ellipseHeight="20" fillColor="0x60FFCC" fillAlpha="0.12"/>
					<e:Label x="30" width="240" verticalAlign="middle" size="34" textColor="0xF0E92F" textAlign="left" top="0" height="80" text="{langData.info3}"/>
					<e:Label x="270" top="0" bottom="0" textAlign="left" size="34" verticalAlign="middle" text="{data.appAddr}" width="560" lineSpacing="10"/>
				</e:Group>
				<e:Group width="860" height="80" x="223" y="252">
					<e:Rect left="0" right="0" top="0" bottom="0" ellipseWidth="20" ellipseHeight="20" fillColor="0x60FFCC" fillAlpha="0.12"/>
					<e:Label x="30" top="0" bottom="0" width="240" verticalAlign="middle" size="34" textColor="0xF0E92F" textAlign="left" text="{langData.info4}"/>
					<e:Label x="270" top="0" bottom="0" verticalAlign="middle" textAlign="left" size="34" text="{data.creatTime}"/>
				</e:Group>
				<e:Group width="860" height="80" x="233" y="262">
					<e:Rect left="0" right="0" top="0" bottom="0" ellipseWidth="20" ellipseHeight="20" fillColor="0x60FFCC" fillAlpha="0.12"/>
					<e:Label x="30" top="0" bottom="0" width="240" verticalAlign="middle" size="34" textColor="0xF0E92F" textAlign="left" text="{langData.info5}"/>
					<e:Label x="270" top="0" bottom="0" verticalAlign="middle" textAlign="left" size="34" text="{data.balance}"/>
				</e:Group>
				<e:Group width="860" height="80" x="243" y="272">
					<e:Rect left="0" right="0" top="0" bottom="0" ellipseWidth="20" ellipseHeight="20" fillColor="0x60FFCC" fillAlpha="0.12"/>
					<e:Label x="30" top="0" bottom="0" width="240" verticalAlign="middle" size="34" textColor="0xF0E92F" textAlign="left" text="{langData.info6}"/>
					<e:Label x="270" top="0" bottom="0" verticalAlign="middle" textAlign="left" size="34" text="{data.historyTotalCoins}"/>
				</e:Group>
				<e:layout>
					<e:VerticalLayout gap="30" verticalAlign="top"/>
				</e:layout>
			</e:Group>
		</e:Scroller>
	</e:Group>
</e:Skin>